<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="360dp"
    android:layout_height="360dp">

    <com.yuyashuai.frameanimation.FrameAnimationView
        android:id="@+id/mWaveBgView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.cyq.progressview.widget.MySmartProgressView
        android:id="@+id/mMySmartProgressView"
        android:layout_width="320dp"
        android:layout_height="320dp"
        android:layout_gravity="center" />

    <com.cyq.progressview.widget.TempNumberView
        android:id="@+id/mTempNumberView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center" />
</FrameLayout>
